{
    "compilerOptions": {
      "target": "ES2021",
      "useDefineForClassFields": true,
      "lib": ["DOM", "ES2021"],
      "jsx": "react-jsx",
      "module": "ESNext",
      "moduleResolution": "Node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "dist"
    },
    "include": ["src"]
  }
  